{
  "name": "ms-groups",
  "version": "1.1.3",
  "description": "Communities microservice",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build:clean",
    "clean": "rm -rf ./dist",
    "test": "exit 0",
    "coverage": "exit 0",
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "build:debug": "npm run clean && npm run build -- --sourceMap",
    "build:clean": "npm run clean && npm run build",
    "build:watch": "tsc -w",
    "start": "node dist/index.js",
    "start:debug": "tsc && concurrently -s all \"tsc -w\" \"DEBUG=expresso:* nodemon dist/index.js\""
  },
  "contributors": [
    "Lucas Santos <lucas@chega.ai>"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@expresso/app": "^1.1.1",
    "@expresso/errors": "^1.3.0",
    "@expresso/server": "^1.1.3",
    "@expresso/validator": "^1.0.5",
    "@nindoo/mongodb-data-layer": "^2.1.0",
    "axios": "^0.19.0",
    "azure-blob-storage-client": "0.0.8",
    "express-rescue": "^1.1.22",
    "reflect-metadata": "^0.1.13",
    "sugar-env": "^1.5.7",
    "tsyringe": "^4.0.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.14",
    "concurrently": "^5.0.0",
    "husky": "^3.1.0",
    "nodemon": "^1.19.4",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "tslint-config-standard": "^9.0.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run build:clean"
    }
  }
}
